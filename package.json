{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "format": "prettier --write \"**/*.+(js|jsx|json|css|md)\""
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.+(js|jsx)": [
      "eslint --fix",
      "git add"
    ],
    "*.+(json|css|md)": [
      "prettier --write",
      "git add"
    ]
  },
  "repository": {
    "type": "github",
    "url": "https://github.com/SalahAdDin/frontend.git"
  },
  "homepage": "https://github.com/SalahAdDin/frontend/#readme",
  "dependencies": {
    "@apollo/react-hooks": "^3.1.5",
    "@apollo/react-ssr": "^3.1.5",
    "apollo-cache-inmemory": "^1.6.6",
    "apollo-client": "^2.6.10",
    "apollo-link-http": "^1.5.17",
    "graphql": "^15.2.0",
    "graphql-tag": "^2.10.3",
    "husky": "^4.2.5",
    "isomorphic-unfetch": "^3.0.0",
    "lint-staged": "^10.2.11",
    "next": "9.4.4",
    "next-with-apollo": "^5.0.1",
    "prettier": "^2.0.5",
    "react": "16.13.1",
    "react-dom": "16.13.1"
  }
}
