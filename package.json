{
  "name": "frontend",
  "version": "2.0.0",
  "private": true,
  "repository": {
    "type": "github",
    "url": "https://github.com/SalahAdDin/frontend.git"
  },
  "homepage": "https://github.com/SalahAdDin/frontend/#readme",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "lint": "next lint",
    "find:unused": "next-unused",
    "format": "prettier --write \"**/*.+(ts|tsx|json|css|scss|md)\"",
    "lint:fix": "eslint --fix './**/*.{ts,tsx}'",
    "prepare": "husky install",
    "start": "next start",
    "type-check": "tsc"
  },
  "lint-staged": {
    "*.+(js|jsx)": [
      "eslint --fix",
      "git add"
    ],
    "*.+(json|css|md)": [
      "prettier --write",
      "git add"
    ]
  },
  "next-unused": {
    "alias": {
      "application": "src/application/",
      "domain": "src/domain/",
      "infrastructure": "src/infrastructure/",
      "pages": "src/pages/",
      "presentation": "src/presentation/"
    },
    "debug": true,
    "include": [
      "application",
      "domain",
      "infrasctructure",
      "pages",
      "presentation"
    ],
    "exclude": [],
    "entrypoints": [
      "src/pages"
    ]
  },
  "dependencies": {
    "@emotion/cache": "^11.4.0",
    "@emotion/react": "^11.4.0",
    "@emotion/server": "^11.4.0",
    "@emotion/styled": "^11.3.0",
    "@material-ui/core": "^5.0.0-alpha.36",
    "@material-ui/icons": "^5.0.0-alpha.36",
    "@material-ui/lab": "^5.0.0-alpha.36",
    "@material-ui/styles": "^5.0.0-alpha.35",
    "clsx": "^1.1.1",
    "dayjs": "^1.10.5",
    "deepmerge": "^4.2.2",
    "next": "11.0.0",
    "next-compose-plugins": "^2.2.1",
    "next-i18next": "^8.5.0",
    "next-offline": "^5.0.5",
    "next-seo": "^4.24.0",
    "next-unused": "^0.0.6",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-markdown": "^6.0.2",
    "react-player": "^2.9.0",
    "react-syntax-highlighter": "^15.4.3",
    "rehype-raw": "^5.1.0",
    "rehype-sanitize": "^4.0.0",
    "remark-unwrap-images": "^2.1.0"
  },
  "devDependencies": {
    "@svgr/webpack": "^5.5.0",
    "@testing-library/react": "^11.2.7",
    "@types/react": "17.0.11",
    "@types/react-dom": "^17.0.8",
    "@types/react-syntax-highlighter": "^13.5.0",
    "@typescript-eslint/eslint-plugin": "^4.26.1",
    "@typescript-eslint/parser": "^4.26.1",
    "eslint": "^7.29.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-next": "^11.0.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prefer-object-spread": "^1.2.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-testing-library": "^4.6.0",
    "husky": "^6.0.0",
    "lint-staged": "^11.0.0",
    "prettier": "^2.3.1",
    "typescript": "^4.3.2"
  }
}
