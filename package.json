{
  "name": "frontend",
  "version": "1.1.1",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "format": "prettier --write \"**/*.+(js|jsx|json|css|md)\"",
    "find:unused": "next-unused",
    "prepare": "husky install && rm -rf .git/hooks && ln -s ../.husky .git/hooks"
  },
  "lint-staged": {
    "*.+(js|jsx)": [
      "eslint --fix",
      "git add"
    ],
    "*.+(json|css|md)": [
      "prettier --write",
      "git add"
    ]
  },
  "next-unused": {
    "alias": {
      "lib": "lib/",
      "assets": "assets/",
      "styles": "styles/",
      "components": "components/"
    },
    "debug": true,
    "include": [
      "assets",
      "components",
      "lib",
      "pages",
      "styles"
    ],
    "exclude": [],
    "entrypoints": [
      "pages"
    ]
  },
  "repository": {
    "type": "github",
    "url": "https://github.com/SalahAdDin/frontend.git"
  },
  "homepage": "https://github.com/SalahAdDin/frontend/#readme",
  "dependencies": {
    "@material-ui/core": "^4.11.2",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.57",
    "@svgr/webpack": "^5.5.0",
    "dayjs": "1.10.4",
    "deepmerge": "^4.2.2",
    "disqus-react": "^1.0.10",
    "fontsource-dm-sans": "4.0.0",
    "fontsource-nunito": "4.0.0",
    "next": "10.1.3",
    "next-compose-plugins": "^2.2.0",
    "next-i18next": "8.2.0",
    "next-offline": "5.0.5",
    "next-seo": "4.24.0",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-markdown": "6.0.0",
    "react-player": "^2.7.2",
    "react-syntax-highlighter": "^15.4.3",
    "rehype-raw": "^5.1.0",
    "rehype-sanitize": "^4.0.0",
    "remark-unwrap-images": "^2.0.0"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "eslint": "7.24.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "8.2.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "3.4.0",
    "eslint-plugin-react": "7.23.2",
    "eslint-plugin-react-hooks": "^4.1.2",
    "husky": "5.1.3",
    "lint-staged": "^10.5.3",
    "next-unused": "0.0.6",
    "prettier": "2.2.1",
    "prop-types": "^15.7.2"
  }
}
